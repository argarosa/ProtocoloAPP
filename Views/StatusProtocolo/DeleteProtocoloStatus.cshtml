@model ProtocoloApp.Models.StatusProtocolo

@{
    ViewData["Title"] = "Excluir Status de Protocolo";
}

<h1>Excluir Status de Protocolo</h1>

<div>
    <h4>Status de Protocolo</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">ID</dt>
        <dd class="col-sm-10">@Model.IdStatus</dd>
        <dt class="col-sm-2">Nome</dt>
        <dd class="col-sm-10">@Model.NomeStatus</dd>
        <dt class="col-sm-2">Descrição</dt>
        <dd class="col-sm-10">@Model.Descricao</dd>
    </dl>
    <form asp-action="DeleteConfirmed">
        <input type="hidden" asp-for="IdStatus" />
        <p>Tem certeza de que deseja excluir este item?</p>
        <button type="submit" class="btn btn-danger">Excluir</button>
        <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
    </form>
</div>